{
  "name": "mwx-limited/cqrs-es-framework-laravel",
  "description": "Laravel adapter for the SmoothPhp CQRS-ES-Framework",
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
    "mwx-limited/cqrs-es-framework": "~2.1.0",
    "illuminate/support": "~5.1",
    "illuminate/database": "~5.1",
    "illuminate/console": "~5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "SmoothPhp\\LaravelAdapter\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SmoothPhp\\LaravelAdapter\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "repositories": [
    {
      "type": "git",
      "name": "mwx-limited/cqrs-es-framework",
      "url": "https://github.com/mwx-limited/CQRS-ES-Framework.git"
    }
  ],
    "minimum-stability": "dev"
}

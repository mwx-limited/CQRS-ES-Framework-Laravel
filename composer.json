{
  "name": "mwx-limited/cqrs-es-framework-laravel",
  "description": "Laravel adapter for the SmoothPhp CQRS-ES-Framework",
  "keywords": [
    "cqrs",
    "laravel",
    "event sourcing"
  ],
  "homepage": "https://github.com/mwx-limited/cqrs-es-framework-laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Bennett",
      "email": "simon@bennett.im",
      "homepage": "https://smoothphp.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "smoothphp/cqrs-es-framework": "~2.0",
    "illuminate/support": "~5.1",
    "illuminate/database": "~5.1",
    "illuminate/console": "~5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "SmoothPhp\\LaravelAdapter\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SmoothPhp\\LaravelAdapter\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
